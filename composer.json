{
    "name": "kitsunetech/velox",
    "description": "Server-side component of Velox framework",
    "type": "library",
    "keywords": ["database","AJAX","JSON"],
    "support": {
        "email": "admin@kitsunetech.com"
    },
    "require": {
        "php": ">=8.0.2",
        "ext-json": "*"
    },
    "suggest": {
        "ext-PDO": "Needed for MySQL/MariaDB support through PDO",
        "ext-pdo_mysql": "Needed for MySQL/MariaDB support through PDO",
        "ext-xmlwriter": "Needed for XML data export"
    },
    "autoload": {
        "psr-4": {
            "KitsuneTech\\Velox\\": "php/"
        },
        "files": ["php/Support/Constants.php","php/Support/ErrorHandling.php","php/Transport/Export.php","config/config.php"]
    }
}
